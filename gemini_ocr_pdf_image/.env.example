# Gemini OCR Configuration
# Copy this file to .env and fill in your values

# Required: Gemini API key
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Processing parameters
LEGIBILITY_THRESHOLD=0.5
SEMANTIC_THRESHOLD=0.6
THINKING_BUDGET=2000
DPI=300

# Optional: Thinking configuration (true/false)
ENABLE_THINKING_ASSESSMENT=true
ENABLE_THINKING_OCR=false

# Optional: Input/Output paths (can still be overridden by CLI)
# INPUT_FILE=path/to/your/file.pdf
# INPUT_DIR=path/to/your/images
# OUTPUT_DIR=./output

# Optional: Processing options
# START_PAGE=1
# END_PAGE=
# SKIP_TEXT_CLEANING=false